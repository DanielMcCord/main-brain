    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch GUI",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "console": "integratedTerminal",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/qode",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/qode.exe"
            }
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Node Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}